{
  "name": "Chrome Chat",
  "version": "1.1",
  "description": "Real Time Anonymous Chat",
  "manifest_version": 2,
  "icons": {},
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "start chatting"
  },
  "permissions": ["storage", "notifications"],

  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
